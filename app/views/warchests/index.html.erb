<p id="notice"><%= notice %></p>

<h1>Warchests</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @warchests.each do |warchest| %>
      <tr>
        <td><%= link_to 'Show', warchest %></td>
        <td><%= link_to 'Edit', edit_warchest_path(warchest) %></td>
        <td><%= link_to 'Destroy', warchest, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Warchest', new_warchest_path %>
