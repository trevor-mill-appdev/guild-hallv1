<p id="notice"><%= notice %></p>

<h1>Mobs</h1>

<table>
  <thead>
    <tr>
      <th>Owner</th>
      <th>Serial</th>
      <th>Image url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mobs.each do |mob| %>
      <tr>
        <td><%= mob.owner_id %></td>
        <td><%= mob.serial %></td>
        <td><%= mob.image_url %></td>
        <td><%= link_to 'Show', mob %></td>
        <td><%= link_to 'Edit', edit_mob_path(mob) %></td>
        <td><%= link_to 'Destroy', mob, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mob', new_mob_path %>
